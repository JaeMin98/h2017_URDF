      </plugin>
    </sensor>
  </link>
</model>

<model name="side_camera">
  <static>true</static>
  <pose>0 -4.0 1 0 -0.081647 1.570793</pose>
  <link name="camera_link">
    <sensor name="camera_sensor" type="camera">
      <!-- 추가된 부분 시작 -->
