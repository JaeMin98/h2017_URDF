</model>

  
    <include>
      <uri>model://ground_plane</uri>
    </include>

<arg name="workspace_path" default="/home/smartcps/catkin_ws/src" />

<model name="table">
  <pose>-0.8 0 0  0 0 0</pose>
  <static>true</static>
  <link name="table">
    <visual name="visual">
      <geometry><mesh><uri>model://table.dae</uri></mesh></geometry>
    </visual>
    <collision name="collision">
      <geometry><mesh><uri>model://table.dae</uri></mesh></geometry>
    </collision>
  </link>
</model>

<model name="cube">
  <pose>-0.8 -0.5 0.826  0 0 0</pose>
  <mass value="1" />

  <link name="cube">
    <visual name="visual">
      <geometry>
        <mesh>
          <uri>model://cube.dae</uri>
          <scale>1.4 1.4 1.4</scale>
        </mesh>
      </geometry>
    </visual>
    <collision name="collision">
      <geometry>
        <mesh>
          <uri>model://cube.dae</uri>
          <scale>1.4 1.4 1.4</scale>
        </mesh>
      </geometry>
    </collision>
  </link>
</model>

